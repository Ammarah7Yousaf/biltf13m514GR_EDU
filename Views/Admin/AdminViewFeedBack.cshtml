@{
    ViewBag.Title = "AdminAddSubject";
    Layout = "~/Views/Shared/_Admin.cshtml";
}
@*============Middle Div about View FeedBack *@

<div class="col-sm-5" style="margin:0px; padding:0px">

    <div class="panel panel-danger" style="margin:80px 0px 0px 80px">

        <div class="panel-heading">
            <span><b>View FeedBack</b></span>
        </div>

        <div class="panel-body">
            <br />
            <form action="/Admin/AdminViewFeedBackNow" class="form-horizontal" role="form">
                <div class="form-group col-sm-12">
                    <label class="control-label col-lg-2" style=" margin-top:8px; padding:0px; color:#204d74">@Model.sender: </label>
                    <div class="col-lg-10"><br /> 
                         @ViewBag.message
                    </div>
                </div>            
            </form>
        </div>
    </div>
</div>

@Scripts.Render("~/bundles/jquery")